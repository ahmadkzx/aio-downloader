{
  "manifest_version": 3,
  "name": "AIO Downloader",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/p/CgcaW8EoXrz/"],
      "js": ["dist/instagram.js"],
      "css": ["dist/instagram.css"]
    }
  ],
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["dist/static/instagram-download.svg"]
}]
}